{
    "name": "Scroll Back",
    "description": "Save your last location on a webpage",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "128": "/icon/default-128.png",
        "48": "/icon/default-48.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "options_page": "settings.html",
    "action": {
    },
    "permissions": [
        "tabs",
        "scripting",
        "storage"
    ],
    "host_permissions": [
        "*://*/*"
    ]
}   